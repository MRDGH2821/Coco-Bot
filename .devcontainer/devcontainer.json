{
  "customizations": {
    "vscode": {
      "extensions": [
        "eamodio.gitlens",
        "editorconfig.editorconfig",
        "esbenp.prettier-vscode",
        "evondev.indent-rainbow-palettes",
        "exiasr.hadolint",
        "foxundermoon.shell-format",
        "oderwat.indent-rainbow",
        "redhat.vscode-yaml",
        "rust-lang.rust-analyzer",
        "spmeesseman.vscode-taskexplorer",
        "streetsidesoftware.code-spell-checker",
        "taiyuuki.vscode-cargo-scripts",
        "tamasfe.even-better-toml",
        "usernamehw.errorlens",
        "vivaxy.vscode-conventional-commits"
      ]
    }
  },
  "features": {
    "ghcr.io/devcontainers-community/npm-features/prettier": {},
    "ghcr.io/devcontainers-extra/features/npm-package": {
      "package": "mega-linter-runner",
      "version": "latest"
    },
    "ghcr.io/devcontainers/features/docker-in-docker": {},
    "ghcr.io/devcontainers/features/github-cli": {},
    "ghcr.io/devcontainers/features/rust": {},
    "ghcr.io/dhoeric/features/hadolint": {},
    "ghcr.io/itsmechlark/features/postgresql": {},
    "ghcr.io/lee-orr/rusty-dev-containers/cargo-binstall": {
      "packages": "cocogitto,cargo-edit"
    },
    "ghcr.io/lee-orr/rusty-dev-containers/cargo-watch": {},
    "ghcr.io/trunk-io/devcontainer-feature/trunk": {}
  },
  "image": "mcr.microsoft.com/devcontainers/rust:latest",
  "mounts": [
    {
      "source": "devcontainer-cargo-cache-${devcontainerId}",
      "target": "/usr/local/cargo",
      "type": "volume"
    }
  ],
  "name": "Coco bot dev container",
  "postCreateCommand": {
    "cocogitto-bootstrap": "bash .devcontainer/cocogitto-bootstrap.sh"
  }
}
